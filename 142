#include <stdio.h>

#define ll long long int

int main() {
    ll n;
    scanf("%lld", &n);

    for (int i = 1; i <= n; i = i + 1) {
        if (i % 2 != 0) {
            for (int j = 1; j <= 2 * i - 1; j = j + 2) {
                printf("%d", j);
            }
            printf("\n");
        } else {
            for (int j = 2; j <= 2 * i; j = j + 2) {
                printf("%d", j);
            }
            printf("\n");
        }
    }

    return 0;
}
