#include <stdio.h>

int main() {
    long long int n, m;
    scanf("%lld %lld", &n, &m);

    if (n<=m)
    {
        for (int i = m; i >=m-n+1; i = i - 1) {
        
            for (int j = i; j > 1; j = j - 1) {
                printf("%d", j);
            }
            for (int k = 1; k <= m - i + 1; k = k + 1) {
                printf("%d", k);
            }
            printf("\n");
        } 
    }

    else 
    {
        for ( int z=n; z>m ;z=z-1 )
        {
            for (int x= z; x>=z-m+1 ;x=x-1)
            {

            
                printf("%d",x);
            }
            printf("\n");
        }
        for (int i = m; i >=m-n+1; i = i - 1) {
        if (i==0)
        {
            break;
        }
            for (int j = i; j > 1; j = j - 1) {
                printf("%d", j);
            }
            for (int k = 1; k <= m - i + 1; k = k + 1) {
                printf("%d", k);
            }
            printf("\n");
        
    }

        

    
    }

}

