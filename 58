#include <stdio.h>
#include <math.h>

#define MAX_PRIME 100000

int main() {
    int t;
    scanf("%d", &t);

    for (int k = 1; k <= t; k++) {
        int n;
        scanf("%d", &n);
        
        int primes[MAX_PRIME] = {0}; 
        int count[MAX_PRIME] = {0}; 
        
        int factor_count = 0;
        
      
        while (n % 2 == 0) {
            primes[factor_count] = 2;
            count[factor_count]++;
            n = n / 2;
        }
        if (count[factor_count] > 0) {
            factor_count++;
        }
        
     
        for (int i = 3; i <= sqrt(n); i = i + 2) {
            while (n % i == 0) {
                primes[factor_count] = i;
                count[factor_count]++;
                n = n / i;
            }
            if (count[factor_count] > 0) {
                factor_count++;
            }
        }
        
     
        if (n > 2) {
            primes[factor_count] = n;
            count[factor_count] = 1;
            factor_count++;
        }
        
        printf("Test %d: ", k);
        for (int i = 0; i < factor_count; i++) {
            printf("%d(%d) ", primes[i], count[i]);
        }
        printf("\n");
    }

    return 0;
}
