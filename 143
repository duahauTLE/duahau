#include <stdio.h>

#define ll long long int

int main() {
    ll n;
    scanf("%lld", &n);

    for (int i = 1; i <= n; i = i + 1) {
        for (int j = 1; j <= 2 * i - 1; j = j + 2) {
            printf("%d", j);
        }
        for (int k = 2 * i - 3; k >= 1; k = k - 2) {
            printf("%d", k);
        }
        printf("\n");
    }

    return 0;
}
