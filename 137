#include <stdio.h>
#include <math.h>

#define ll long long

int is_prime(ll n) {
    if (n <= 1) return 0;
    for (ll i = 2; i <= sqrt(n); i++) {
        if (n % i == 0) return 0;
    }
    return 1;
}

ll compute(ll n) {
    ll result = 1;
    for (ll i = 2; i * i <= n; i++) {
        if (n % i == 0) {
            while (n % i == 0) {
                n /= i;
            }
            result *= i;
        }
    }
    if (n > 1) result *= n;
    return result;
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        ll n;
        scanf("%lld", &n);
        ll ans = compute(n);
        printf("%lld\n", ans);
    }
    return 0;
}
